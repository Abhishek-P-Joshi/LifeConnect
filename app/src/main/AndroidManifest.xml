<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amank.lifeconnect">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".DoctorDashboard"/>
        <activity android:name=".SelectPatient"/>
        <activity android:name=".CaptureLogin"/>
        <activity android:name=".CaptureRegistration"/>
        <activity android:name=".UploadLogin"/>
        <activity android:name=".UploadRegistration"/>
        <activity
            android:name=".Patient_Dashboard"
            android:label="@string/title_activity_patient__dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DemoInsert"
            android:label="@string/title_activity_demo_insert"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DemoGet"
            android:label="@string/title_activity_demo_get"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <service android:enabled="true" android:name=".SensorHandler" />
    </application>

</manifest>